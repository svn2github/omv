<VirtualHost *:80>
	Include /etc/apache2/openmediavault-webgui.conf
	SSLEngine off
</Virtualhost>
<VirtualHost *:443>
	Include /etc/apache2/openmediavault-webgui.conf
	SSLEngine on
	SSLCertificateFile /etc/ssl/certs/openmediavault.crt
	SSLCertificateKeyFile /etc/ssl/private/openmediavault.key
</Virtualhost>
